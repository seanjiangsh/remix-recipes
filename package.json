{
  "name": "remix-recipes",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix dev --manual -c \"arc sandbox -p 3000\"",
    "dev:db-dump": "npx tsx app/utils/ddb/db-dump.ts",
    "build": "tsc && remix build",
    "start": "remix-serve ./build/index.js",
    "test:e2e": "cross-env INCLUDE_TEST_ROUTES=true npx playwright test",
    "test:e2e:ui": "cross-env INCLUDE_TEST_ROUTES=true npx playwright test --ui",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "deploy": "npm run build && arc deploy --production",
    "destroy": "arc destroy --app remix-recipes --production --force",
    "seed:dev": "cross-env ARC_ENV=testing tsx app/utils/ddb/seeding/seed.ts",
    "seed:prod": "cross-env ARC_ENV=production tsx app/utils/ddb/seeding/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.592.0",
    "@getbrevo/brevo": "2.1.1",
    "@prisma/client": "5.15.0",
    "@remix-run/architect": "^2.9.2",
    "@remix-run/css-bundle": "2.9.2",
    "@remix-run/node": "2.9.2",
    "@remix-run/react": "2.9.2",
    "@remix-run/serve": "2.9.2",
    "classnames": "^2.5.1",
    "cryptr": "^6.3.0",
    "dynamoose": "^4.0.1",
    "fs-extra": "^11.2.0",
    "isbot": "5.1.9",
    "mime": "^4.0.3",
    "prisma": "5.15.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-modal": "^3.16.1",
    "tailwindcss": "3.4.4",
    "uuid": "^9.0.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@architect/architect": "11.0.13",
    "@architect/functions": "8.1.6",
    "@architect/plugin-typescript": "^2.1.0",
    "@playwright/test": "^1.44.1",
    "@remix-run/dev": "2.9.2",
    "@types/cryptr": "^4.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-modal": "^3.16.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "cross-env": "^7.0.3",
    "eslint": "9.0.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "7.34.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "tsx": "4.14.1",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
